                <metric alias="CACHE.searcher.perSegFilter.evictions" attr="CACHE.searcher.perSegFilter.evictions" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="INDIVIDUAL" />
        </mBeanGroup>
        <mBeanGroup category="CACHE" subCategory="queryResultCache" metricSection="stats">
                <metric alias="CACHE.searcher.queryResultCache.cumulative_inserts" attr="CACHE.searcher.queryResultCache.cumulative_inserts" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="INDIVIDUAL" />
                <metric alias="CACHE.searcher.queryResultCache.evictions" attr="CACHE.searcher.queryResultCache.evictions" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="INDIVIDUAL" />
                <metric alias="CACHE.searcher.queryResultCache.lookups" attr="CACHE.searcher.queryResultCache.lookups" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="INDIVIDUAL" />
        </mBeanGroup>
    </stat>


